    "from collections import defaultdict\n",
    "n = int(input())\n",
    "\n",
    "table = defaultdict(lambda: 10**10)\n",
    "\n",
    "for _ in range(n):\n",
    "    a,c = map(int,input().split())\n",
    "    table[c] = min(table[c],a)\n",
    "\n",
    "ans = 0\n",
    "for key in table.keys():\n",
    "    ans = max(table[key],ans)\n",
    "\n",
    "print(ans)"
