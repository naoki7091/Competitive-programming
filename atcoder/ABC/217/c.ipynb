    "n = int(input())\n",
    "\n",
    "p = list(map(int,input().split()))\n",
    "\n",
    "p_dict = [0]*(n + 1)\n",
    "\n",
    "for i in range(n):\n",
    "    p_dict[p[i]] = i + 1\n",
    "\n",
    "# print(p_dict)\n",
    "\n",
    "ans = []\n",
    "for num in p_dict:\n",
    "    ans.append(num)\n",
    "\n",
    "print(*ans[1:])"
   ]
